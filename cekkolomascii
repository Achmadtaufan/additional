DECLARE @TableName SYSNAME = 'NamaTabelKamu';
DECLARE @SQL NVARCHAR(MAX) = N'';

SELECT @SQL = @SQL + '
SELECT ''' + c.name + ''' AS ColumnName, *
FROM ' + QUOTENAME(s.name) + '.' + QUOTENAME(t.name) + '
WHERE TRY_CAST(' + QUOTENAME(c.name) + ' AS NVARCHAR(MAX)) LIKE ''%[' +
    CHAR(0) + CHAR(1) + CHAR(2) + CHAR(3) + CHAR(4) + CHAR(5) +
    CHAR(6) + CHAR(7) + CHAR(8) + CHAR(11) + CHAR(12) +
    CHAR(14) + CHAR(15) + CHAR(16) + CHAR(17) + CHAR(18) +
    CHAR(19) + CHAR(20) + CHAR(21) + CHAR(22) + CHAR(23) +
    CHAR(24) + CHAR(25) + CHAR(26) + CHAR(27) + CHAR(28) +
    CHAR(29) + CHAR(30) + CHAR(31) + ']%''
UNION ALL
'
FROM sys.columns c
JOIN sys.tables t ON c.object_id = t.object_id
JOIN sys.schemas s ON t.schema_id = s.schema_id
WHERE t.name = @TableName
  AND c.system_type_id IN (167, 175, 231, 239); -- varchar, char, nvarchar, nchar

-- hapus UNION ALL terakhir
SET @SQL = LEFT(@SQL, LEN(@SQL) - 10);

EXEC sp_executesql @SQL;