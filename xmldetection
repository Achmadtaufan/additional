SELECT 
    t.*,
    i.n AS Position,
    SUBSTRING(t.placeofbirth, i.n, 1) AS IllegalChar,
    UNICODE(SUBSTRING(t.placeofbirth, i.n, 1)) AS IllegalCharCode,
    master..fn_varbintohexstr(CONVERT(varbinary(10), SUBSTRING(t.placeofbirth, i.n, 1))) AS IllegalCharHex
FROM ztemp_test t
CROSS APPLY (
    SELECT number AS n
    FROM master..spt_values
    WHERE type = 'P' AND number BETWEEN 1 AND LEN(t.placeofbirth)
) AS i
WHERE UNICODE(SUBSTRING(t.placeofbirth, i.n, 1)) NOT IN (9,10,13)
  AND (
        UNICODE(SUBSTRING(t.placeofbirth, i.n, 1)) < 32
     OR (UNICODE(SUBSTRING(t.placeofbirth, i.n, 1)) BETWEEN 55296 AND 57343)
     OR (UNICODE(SUBSTRING(t.placeofbirth, i.n, 1)) > 65533)
  );