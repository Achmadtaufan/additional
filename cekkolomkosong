SELECT 
    t.placeofbirth,
    CAST(
        STRING_AGG(
            CONVERT(NVARCHAR(10), UNICODE(SUBSTRING(t.placeofbirth, v.number, 1))), 
            ','
        ) 
        WITHIN GROUP (ORDER BY v.number)
        AS NVARCHAR(MAX)
    ) AS UnicodeList
FROM YourTable t
JOIN master..spt_values v 
    ON v.type = 'P' 
   AND v.number BETWEEN 1 AND LEN(t.placeofbirth)
GROUP BY t.placeofbirth
HAVING MIN(UNICODE(SUBSTRING(t.placeofbirth, v.number, 1))) < 32
    OR MAX(UNICODE(SUBSTRING(t.placeofbirth, v.number, 1))) > 126;