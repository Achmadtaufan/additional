SELECT
    t1.ID,
    t1.Col1,
    t2.Col2,
    c.CleanPlaceOfBirth
FROM Table1 t1
JOIN Table2 t2 ON t1.ID = t2.T1ID
CROSS APPLY
(
    SELECT STRING_AGG(
             CASE 
                 WHEN SUBSTRING(t1.PlaceOfBirth, v.number, 1) LIKE '[A-Za-z ]'
                 THEN SUBSTRING(t1.PlaceOfBirth, v.number, 1)
                 ELSE ' '
             END, ''
           ) WITHIN GROUP (ORDER BY v.number) AS CleanPlaceOfBirth
    FROM master..spt_values v
    WHERE v.type = 'P'
      AND v.number BETWEEN 1 AND LEN(t1.PlaceOfBirth)
) c;