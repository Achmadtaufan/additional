DECLARE @search VARCHAR(200);

DECLARE curSearch CURSOR FOR
SELECT search_value FROM #TempTable;

OPEN curSearch;
FETCH NEXT FROM curSearch INTO @search;

WHILE @@FETCH_STATUS = 0
BEGIN
    SELECT
        b.name AS job_name,
        a.step_id,
        a.step_name,
        a.command,
        @search AS search_used
    FROM msdb.dbo.sysjobsteps a
    JOIN msdb.dbo.sysjobs b 
        ON a.job_id = b.job_id
    WHERE a.command LIKE '%' + @search + '%';

    FETCH NEXT FROM curSearch INTO @search;
END

CLOSE curSearch;
DEALLOCATE curSearch;